package pl.ingenico.automation.model;

public class UserResponse {

    private String id;
    private String name;
    private String job;
    private String createdAt;

    public String getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public String getJob() {
        return job;
    }
}



